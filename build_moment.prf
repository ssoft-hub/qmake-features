# Formatted date and time decimal values
# Alternative of _DATE_

win32 | win64 \
{
    defineReplace(currentDate) \
    {
        return( $$system( date /t ) )
    }
}

unix \
{
    defineReplace(currentDate) \
    {
        return( $$system( date "+%d.%m.%Y" ) )
    }
}

win32 | win64 \
{
    defineReplace(currentTime) \
    {
        return( $$system( time /T ) )
    }
} \

unix \
{
    defineReplace(currentTime) \
    {
        return( $$system( date "+%H:%M:%S" ) )
    }
}
